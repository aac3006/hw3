<%# Place details page %>
<h1><%= @place["name"]%></h1>

<ul>
    <% for post in @posts %>
    <h2>
        <strong><%= post["title"] %></strong>
    </h2>
    <p>
        <%= post["posted_on"] %>
    </p>
    <p>
        <%= post["description"] %>
    </p>
    <% end %>
</ul>


<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>


<p>
    <a href="/places">Back to all places</a>
</p>